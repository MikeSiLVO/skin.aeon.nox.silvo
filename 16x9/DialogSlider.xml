<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>11</defaultcontrol>
	<animation type="WindowOpen" reversible="false">
		<effect type="slide" start="0,-196" end="0" tween="cubic" easing="inout" time="400" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="slide" end="0,-196" start="0" tween="cubic" easing="inout" time="400" />
	</animation>
	<coordinates>
		<left>600</left>
	</coordinates>
	<controls>
		<control type="image">
			<width>720</width>
			<height>140</height>
			<texture border="40,0,40,35">topbar2.png</texture>
		</control>
		<control type="label" id="10">
			<description>Dialog Header</description>
			<left>30</left>
			<width>345</width>
			<height>60</height>
			<textoffsetx>15</textoffsetx>
			<font>font14_title</font>
			<textcolor>grey2</textcolor>
		</control>
		<control type="label" id="12">
			<description>Slider Value</description>
			<left>30</left>
			<width>650</width>
			<height>60</height>
			<align>right</align>
			<font>font14_title</font>
			<textcolor>grey2</textcolor>
		</control>
		<control type="slider" id="11">
			<description>Slider</description>
			<left>45</left>
			<top>60</top>
			<width>630</width>
			<height>30</height>
			<texturesliderbar>osd_slider_bg.png</texturesliderbar>
			<textureslidernib>osd_slider_nibNF.png</textureslidernib>
			<textureslidernibfocus colordiffuse="$VAR[ThemeLabelColor]">osd_slider_nib.png</textureslidernibfocus>
			<onup>11</onup>
			<ondown>11</ondown>
			<onleft>11</onleft>
			<onright>11</onright>
		</control>
		<include>DebugWindow</include>
	</controls>
</window>