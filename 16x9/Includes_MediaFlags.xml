			<control type="group" id="9506">
				<width>170</width>
				<visible>![Container.Content(movies) + Control.IsVisible(50)]</visible>
				<visible>![Container.Content(movies) + Skin.HasSetting(DisableFlag_MovieStudio)]</visible>
				<visible>![Container.Content(episodes) + Skin.HasSetting(DisableFlag_EpisodeStudio)]</visible>
				<control type="image" id="99025">
					<include>MediaFlagValues</include>
					<texture fallback="colors/blank.png" background="true">$VAR[PathFlagsStudios]$INFO[ListItem.Studio,,.png]</texture>
				</control>
				<control type="label">
					<width>170</width>
					<height>78</height>
					<align>center</align>
					<font>font10</font>
					<textcolor>grey</textcolor>
					<wrapmultiline>true</wrapmultiline>
					<label>$INFO[ListItem.Studio]</label>
