			<!-- Audio Channels -->
			<control type="image" id="9513">
				<include>MediaFlagValues</include>
				<texture>$VAR[MediaFlagsPathVar]/audiochannels/$INFO[ListItem.AudioChannels,,.png]</texture>
				<visible>![Container.Content(movies) + Skin.HasSetting(DisableFlag_MovieAudioChannels)]</visible>
				<visible>![Container.Content(episodes) + Skin.HasSetting(DisableFlag_EpisodeAudioChannels)]</visible>
			</control>
			<!-- LANGUAGE FLAGS -->
			<control type="grouplist" id="9514">
				<width>170</width>
				<height>78</height>
				<align>center</align>
				<visible>!String.IsEmpty(Skin.String(LanguageFlags)) + [Container.Content(movies) | Container.Content(episodes)]</visible>
				<!-- Audio Language -->
				<control type="grouplist">
					<width>170</width>
					<height>30</height>
					<orientation>horizontal</orientation>
					<itemgap>5</itemgap>
					<visible>String.IsEqual(Skin.String(LanguageFlags),1) | String.IsEqual(Skin.String(LanguageFlags),3)</visible>
					<control type="image">
						<width>28</width>
						<height>28</height>
						<aspectratio>keep</aspectratio>
						<colordiffuse>grey</colordiffuse>
						<texture>flags/audio.png</texture>
						<visible>!String.IsEmpty(ListItem.Property(AudioLanguage.1)) | !String.IsEmpty(ListItem.Property(AudioLanguage.2)) | !String.IsEmpty(ListItem.Property(AudioLanguage.3))</visible>
					</control>
					<control type="image">
						<width>30</width>
						<height>30</height>
						<aspectratio>keep</aspectratio>
						<colordiffuse>grey</colordiffuse>
						<texture>$VAR[PathFlagsLanguage]$INFO[ListItem.Property(AudioLanguage.1),,.png]</texture>
						<visible>!String.IsEmpty(ListItem.Property(AudioLanguage.1))</visible>
					</control>
					<control type="image">
						<width>30</width>
						<height>30</height>
						<aspectratio>keep</aspectratio>
						<colordiffuse>grey</colordiffuse>
						<texture>$VAR[PathFlagsLanguage]$INFO[ListItem.Property(AudioLanguage.2),,.png]</texture>
						<visible>!String.IsEmpty(ListItem.Property(AudioLanguage.2))</visible>
					</control>
					<control type="image">
						<width>30</width>
						<height>30</height>
						<aspectratio>keep</aspectratio>
						<colordiffuse>grey</colordiffuse>
						<texture>$VAR[PathFlagsLanguage]$INFO[ListItem.Property(AudioLanguage.3),,.png]</texture>
						<visible>!String.IsEmpty(ListItem.Property(AudioLanguage.3))</visible>
					</control>
				</control>
				<!-- Subtitle Language -->
				<control type="grouplist">
					<top>30</top>
					<width>170</width>
					<height>30</height>
					<orientation>horizontal</orientation>
					<itemgap>5</itemgap>
					<visible>String.IsEqual(Skin.String(LanguageFlags),2) | String.IsEqual(Skin.String(LanguageFlags),3)</visible>
					<control type="image">
						<width>28</width>
						<height>28</height>
						<aspectratio>keep</aspectratio>
						<colordiffuse>grey</colordiffuse>
						<texture>flags/subtitles.png</texture>
						<visible>!String.IsEmpty(ListItem.Property(SubtitleLanguage.1)) | !String.IsEmpty(ListItem.Property(SubtitleLanguage.2)) | !String.IsEmpty(ListItem.Property(SubtitleLanguage.3))</visible>
					</control>
					<control type="image">
						<width>30</width>
						<height>30</height>
						<aspectratio>keep</aspectratio>
						<colordiffuse>grey</colordiffuse>
						<texture>$VAR[PathFlagsLanguage]$INFO[ListItem.Property(SubtitleLanguage.1),,.png]</texture>
						<visible>!String.IsEmpty(ListItem.Property(SubtitleLanguage.1))</visible>
					</control>
					<control type="image">
						<width>30</width>
						<height>30</height>
						<aspectratio>keep</aspectratio>
						<colordiffuse>grey</colordiffuse>
						<texture>$VAR[PathFlagsLanguage]$INFO[ListItem.Property(SubtitleLanguage.2),,.png]</texture>
						<visible>!String.IsEmpty(ListItem.Property(SubtitleLanguage.2))</visible>
					</control>
					<control type="image">
						<width>30</width>
						<height>30</height>
						<aspectratio>keep</aspectratio>
						<colordiffuse>grey</colordiffuse>
						<texture>$VAR[PathFlagsLanguage]$INFO[ListItem.Property(SubtitleLanguage.3),,.png]</texture>
						<visible>!String.IsEmpty(ListItem.Property(SubtitleLanguage.3))</visible>
					</control>
				</control>
			</control>
			<control type="image">
				<visible>$EXP[HasOnlyOneRating] + ![Control.IsVisible(56) | Control.IsVisible(57) | Control.IsVisible(506)]</visible>
				<width>170</width>
				<height>78</height>
				<texture>colors/blank.png</texture>
			</control>
		</control>
	</include>
	<include name="MediaFlagsSets">
		<control type="grouplist">
			<animation effect="slide" end="390" condition="Control.IsVisible(57)">Conditional</animation>
			<animation effect="slide" end="1150,0" condition="Control.IsVisible(56) | Control.IsVisible(506)">Conditional</animation>
			<animation effect="slide" end="170,0" condition="!Control.IsVisible(9520) + [Control.IsVisible(56) | Control.IsVisible(506)]">Conditional</animation>
			<animation effect="slide" end="170,0" condition="!Control.IsVisible(9521) + [Control.IsVisible(56) | Control.IsVisible(506)]">Conditional</animation>
			<animation effect="slide" end="170,0" condition="!Control.IsVisible(9522) + [Control.IsVisible(56) | Control.IsVisible(506)]">Conditional</animation>
			<animation effect="slide" end="170,0" condition="!Control.IsVisible(9523) + [Control.IsVisible(56) | Control.IsVisible(506)]">Conditional</animation>
			<left>80</left>
			<bottom>14</bottom>
			<width>680</width>
			<height>78</height>
			<orientation>horizontal</orientation>
			<visible>[Control.IsVisible(50) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableList_Movieflags)] | [Control.IsVisible(55) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableTriPanel_Movieflags)] | [Control.IsVisible(56) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisablePosters_Movieflags)] | [Control.IsVisible(57) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableShift_Movieflags)] | [Control.IsVisible(58) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableBannerWall_Movieflags)] | [Control.IsVisible(500) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableIcons_Movieflags)] | [Control.IsVisible(506) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableRightList_Movieflags)] | [Control.IsVisible(509) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableMyFlix_Movieflags)] | [Control.IsVisible(512) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableBigFan_Movieflags)] | [Control.IsVisible(510) + [Container.Content(movies) | Container.Content(sets)] + !Skin.HasSetting(DisableFanartList_Movieflags)]</visible>
			<visible>ListItem.IsCollection</visible>
			<animation effect="fade" time="300" delay="200">Visible</animation>
			<!-- Set Rating -->
			<control type="group" id="9520">
				<visible>!String.IsEmpty(ListItem.Rating) + !Skin.HasSetting(DisableFlag_SetRating)</visible>
				<width>170</width>
				<control type="image">
					<width>60</width>
					<include>MediaFlagValues</include>
					<texture>flags/sets_rating.png</texture>
				</control>
				<control type="group">
					<left>62</left>
					<width>110</width>
					<height>78</height>
					<control type="label">
						<top>10</top>
						<height>39</height>
						<font>font13_bold</font>
						<textcolor>grey</textcolor>
						<label>$INFO[ListItem.Rating]</label>
					</control>
					<control type="label">
						<top>34</top>
						<height>39</height>
						<font>font10</font>
						<textcolor>grey</textcolor>
						<label>[LOWERCASE]$LOCALIZE[563][/LOWERCASE]</label>
					</control>
				</control>
			</control>
			<!-- Set Count -->
			<control type="group" id="9521">
				<width>170</width>
				<visible>!String.IsEqual(Container(4500).NumItems,0) + !Skin.HasSetting(DisableFlag_SetCount)</visible>
				<control type="image">
					<top>-14</top>
					<width>106</width>
					<height>106</height>
					<include>MediaFlagValues</include>
					<texture>views/list/DefaultSets.png</texture>
				</control>
				<control type="group">
					<left>90</left>
					<width>110</width>
					<height>78</height>
					<control type="label">
						<top>10</top>
						<height>39</height>
						<font>font13_bold</font>
						<textcolor>grey</textcolor>
						<label>$INFO[Container(4500).NumItems]</label>
					</control>
					<control type="label">
						<top>34</top>
						<height>39</height>
						<font>font10</font>
						<textcolor>grey</textcolor>
						<label>$LOCALIZE[36901]</label>
					</control>
				</control>
			</control>
			<!-- Studio -->
			<control type="group" id="9522">
				<width>170</width>
				<visible>!String.IsEmpty(Container(4500).ListItem(0).Studio) + !Skin.HasSetting(DisableFlag_SetStudio)</visible>
				<control type="image" id="95220">
					<include>MediaFlagValues</include>
					<texture>$VAR[PathFlagsStudios]$INFO[Container(4500).ListItem(0).Studio,,.png]</texture>
				</control>
				<control type="label">
					<width>170</width>
					<height>78</height>
					<align>center</align>
					<label>$INFO[Container(4500).ListItem(0).Studio]</label>
					<font>font10</font>
					<textcolor>grey2</textcolor>
					<wrapmultiline>true</wrapmultiline>
					<visible>String.IsEmpty(Control.GetLabel(95220))</visible>
				</control>
			</control>
			<!-- MPAA -->
			<control type="group" id="9523">
				<width>170</width>
				<visible>!Skin.HasSetting(DisableFlag_SetMPAA)</visible>
				<control type="image">
					<include>MediaFlagValues</include>
					<texture>$VAR[MediaFlagsPathVar]/mpaa/$VAR[SetRatingFlagVar]</texture>
				</control>
			</control>
		</control>
	</include>
	<!-- <include name="MediaFlagsSplit"></include> -->
	<include name="MediaFlagsTV">
		<control type="group">
			<left>660</left>
			<bottom>14</bottom>
			<width>1190</width>
			<height>78</height>
			<visible>[Container.Content(tvshows) | Container.Content(seasons)] + [[Control.IsVisible(50) + [Container.Content(tvshows) + !Skin.HasSetting(DisableList_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisableList_Seasonflags)]] | [Control.IsVisible(55) + [Container.Content(tvshows) + !Skin.HasSetting(DisableTriPanel_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisableTriPanel_Seasonflags)]] | [Control.IsVisible(56) + [Container.Content(tvshows) + !Skin.HasSetting(DisablePosters_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisablePosters_Seasonflags)]] | [Control.IsVisible(57) + [Container.Content(tvshows) + !Skin.HasSetting(DisableShift_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisableShift_Seasonflags)]] | [Control.IsVisible(58) + !Skin.HasSetting(DisableBannerWall_TVflags)] | [Control.IsVisible(500) + [Container.Content(tvshows) + !Skin.HasSetting(DisableIcons_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisableIcons_Seasonflags)]] | [Control.IsVisible(506) + [Container.Content(tvshows) + !Skin.HasSetting(DisableRightList_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisableRightList_Seasonflags)]] | [Control.IsVisible(509) + [Container.Content(tvshows) + !Skin.HasSetting(DisableMyFlix_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisableMyFlix_Seasonflags)]] | [Control.IsVisible(512) + [Container.Content(tvshows) + !Skin.HasSetting(DisableBigFan_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisableBigFan_Seasonflags)]] | [Control.IsVisible(510) + [Container.Content(tvshows) + !Skin.HasSetting(DisableFanartList_TVflags)] | [Container.Content(seasons) + !Skin.HasSetting(DisableFanartList_Seasonflags)]]]</visible>
			<animation effect="fade" time="300" delay="200">Visible</animation>
			<!-- RATINGS (TV shows only) -->
			<control type="group">
				<left>0</left>
				<width>340</width>
				<height>78</height>
				<visible>Container.Content(tvshows)</visible>
				<include>RatingsMediaFlags</include>
			</control>
			<!-- STUDIO -->
			<control type="group">
				<left>340</left>
				<width>170</width>
				<height>78</height>
				<visible>[[Container.Content(tvshows) + !Control.IsVisible(50)] | Container.Content(seasons)] + !String.IsEmpty(ListItem.Studio) + !Skin.HasSetting(DisableFlag_TVStudio)</visible>
				<control type="image" id="99026">
					<include>MediaFlagValues</include>
					<texture>$VAR[PathFlagsStudios]$INFO[ListItem.Studio,,.png]</texture>
				</control>
				<control type="label">
					<width>170</width>
					<height>78</height>
					<align>center</align>
					<font>font10</font>
					<textcolor>grey</textcolor>
					<wrapmultiline>true</wrapmultiline>
					<label>$INFO[ListItem.Studio]</label>
					<visible>String.IsEmpty(Control.GetLabel(99026))</visible>
				</control>
			</control>
			<!-- MPAA -->
			<control type="image">
				<left>510</left>
				<include>MediaFlagValues</include>
				<texture fallback="flags/default/mpaa/NR.png">$VAR[MediaFlagsPathVar]/mpaa/$VAR[RatingFlagVar]</texture>
				<visible>![String.IsEmpty(ListItem.MPAA) + String.StartsWith(Container.FolderPath,plugin://)] + !Skin.HasSetting(DisableFlag_TVMPAA)</visible>
			</control>
			<!-- SEASONS COUNT (TV shows only) -->
			<control type="group">
				<left>680</left>
				<width>170</width>
				<height>78</height>
				<visible>Container.Content(tvshows) + !String.IsEmpty(ListItem.Property(TotalSeasons)) + !Skin.HasSetting(DisableFlag_TVSeasons)</visible>
				<control type="label">
					<height>50</height>
					<font>font48_bold</font>
					<textcolor>grey</textcolor>
					<align>center</align>
					<wrapmultiline>true</wrapmultiline>
					<label>$INFO[ListItem.Property(TotalSeasons)]</label>
				</control>
				<control type="label">
					<top>42</top>
					<height>30</height>
					<align>center</align>
					<label>[LOWERCASE]$VAR[OneOrMoreSeasonsVar][/LOWERCASE]</label>
					<font>font12</font>
					<textcolor>grey</textcolor>
				</control>
			</control>
			<!-- TOTAL EPISODES -->
			<control type="group">
				<left>850</left>
				<width>170</width>
				<height>78</height>
				<visible>!String.IsEmpty(ListItem.Property(TotalEpisodes)) + !Skin.HasSetting(DisableFlag_TVEpisodes)</visible>
				<control type="label">
					<height>50</height>
					<font>font48_bold</font>
					<textcolor>grey</textcolor>
					<align>center</align>
					<wrapmultiline>true</wrapmultiline>
					<label>$INFO[ListItem.Property(TotalEpisodes)]</label>
				</control>
				<control type="label">
					<top>42</top>
					<height>30</height>
					<font>font12</font>
					<textcolor>grey</textcolor>
					<align>center</align>
					<label>[LOWERCASE]$VAR[OneOrMoreEpisodesVar][/LOWERCASE]</label>
				</control>
			</control>
			<!-- UNWATCHED EPISODES -->
			<control type="group">
				<left>1020</left>
				<width>170</width>
				<height>78</height>
				<visible>!String.IsEmpty(ListItem.Property(UnWatchedEpisodes)) + !Skin.HasSetting(DisableFlag_TVUnWatchedEpisodes)</visible>
				<control type="label">
					<height>50</height>
					<font>font48_bold</font>
					<textcolor>$VAR[UnWatchedEpColor]</textcolor>
					<align>center</align>
					<wrapmultiline>true</wrapmultiline>
					<label>$INFO[ListItem.Property(UnWatchedEpisodes)]</label>
				</control>
				<control type="label">
					<top>42</top>
					<height>30</height>
					<font>font12</font>
