					<textcolor>grey2</textcolor>
					<focusedcolor>white</focusedcolor>
					<align>center</align>
					<onleft>noop</onleft>
					<onright>noop</onright>
					<onup>noop</onup>
					<ondown>noop</ondown>
					<onclick>Dialog.Close(1126)</onclick>
