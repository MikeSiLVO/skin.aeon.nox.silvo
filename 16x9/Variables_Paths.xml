<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<variable name="CustomInfoDetailsPathVar">
		<value condition="!Window.IsActive(1102)">special://skin/extras/playlists/empty.xsp</value>
		<value>plugin://script.skin.info.service/?action=getdetails&amp;dbid=$INFO[Window(Home).Property(item_dbid)]&amp;dbtype=$INFO[Window(Home).Property(item_dbtype)]&amp;reload=$INFO[Skin.TimerElapsedSecs(resetfocus1102)]</value>
	</variable>
	<variable name="SkinInfoLibraryPathVar">
		<value condition="Window.IsActive(fullscreenvideo) + VideoPlayer.Content(movies) + !String.IsEmpty(VideoPlayer.DBID)">plugin://script.skin.info.service/?action=getdetails&amp;dbid=$INFO[VideoPlayer.DBID]&amp;dbtype=movie</value>
		<value condition="Window.IsActive(fullscreenvideo) + VideoPlayer.Content(episodes) + !String.IsEmpty(VideoPlayer.DBID)">plugin://script.skin.info.service/?action=getdetails&amp;dbid=$INFO[VideoPlayer.DBID]&amp;dbtype=episode</value>
		<value condition="!String.IsEmpty(ListItem.DBID)">plugin://script.skin.info.service/?action=getdetails&amp;dbid=$INFO[ListItem.DBID]&amp;dbtype=$INFO[ListItem.DBTYPE]</value>
	</variable>
	<variable name="SkinInfoOnlinePathVar">
		<!-- <value condition="!Window.IsActive(movieinformation)">special://skin/extras/playlists/empty.xsp</value> -->
		<value>$INFO[ListItem.DBID,plugin://script.skin.info.service/?action=online&amp;dbid=]$INFO[ListItem.DBTYPE,&amp;dbtype=]</value>
	</variable>
	<variable name="SkinInfoPlayerOnlinePathVar">
		<value condition="VideoPlayer.Content(movies) + !String.IsEmpty(VideoPlayer.DBID)">plugin://script.skin.info.service/?action=online&amp;dbid=$INFO[VideoPlayer.DBID]&amp;dbtype=movie</value>
		<value condition="VideoPlayer.Content(episodes) + !String.IsEmpty(VideoPlayer.DBID)">plugin://script.skin.info.service/?action=online&amp;dbid=$INFO[VideoPlayer.DBID]&amp;dbtype=episode</value>
	</variable>
	<variable name="SkinInfoCastPathVar">
		<!-- <value condition="!Window.IsActive()">special://skin/extras/playlists/empty.xsp</value> -->
		<value condition="Skin.HasSetting(OnlineCast) + Window.IsVisible(fullscreeninfo)">plugin://script.skin.info.service/?action=get_cast&amp;dbid=$INFO[VideoPlayer.DBID]&amp;dbtype=$VAR[VideoPlayerDBTYPEVar]&amp;online=true</value>
		<value condition="Window.IsVisible(fullscreeninfo)">plugin://script.skin.info.service/?action=get_cast&amp;dbid=$INFO[VideoPlayer.DBID]&amp;dbtype=$VAR[VideoPlayerDBTYPEVar]</value>
		<value condition="Skin.HasSetting(OnlineCast)">plugin://script.skin.info.service/?action=get_cast&amp;dbid=$INFO[ListItem.DBID]&amp;dbtype=$INFO[ListItem.DBTYPE]&amp;online=true</value>
		<value>plugin://script.skin.info.service/?action=get_cast&amp;dbid=$INFO[ListItem.DBID]&amp;dbtype=$INFO[ListItem.DBTYPE]</value>
	</variable>
	<variable name="ShortcutsViewContentVar">
		<value condition="String.IsEqual(Container.Content,genres) + Window.IsVisible(MyMusicNav.xml)">genres-music</value>
		<value condition="String.StartsWith(Container.FolderPath,videodb://movies/sets) + !Container.Content(videoversions) + !Container.Content(sets)">movies-sets</value>
		<value condition="String.StartsWith(Container.FolderPath,videodb://movies/videoversions)">videodb-versions</value>
		<value condition="!String.IsEmpty(Container.Content)">$INFO[Container.Content]</value>
		<value condition="Container.Content() + Window.IsVisible(MyVideoNav.xml)">none-videos</value>
	</variable>
	<variable name="CustomVideoInfoCastPanelContentVar">
		<value condition="Skin.HasSetting(OnlineCast)">plugin://script.skin.info.service/?action=get_cast&amp;dbid=$INFO[Container(4800).ListItem.DBID]&amp;dbtype=$INFO[Container(4800).ListItem.DBTYPE]&amp;online=true</value>
		<value>plugin://script.skin.info.service/?action=get_cast&amp;dbid=$INFO[Container(4800).ListItem.DBID]&amp;dbtype=$INFO[Container(4800).ListItem.DBTYPE]</value>
	</variable>
	<variable name="MovieSetsPathVar">
		<value condition="String.IsEqual(ListItem.DBTYPE,set)">videodb://movies/sets/$INFO[ListItem.DBID]/?setid=$INFO[ListItem.DBID]</value>
	</variable>
	<variable name="JukeBoxContentPathVar">
		<value condition="Container.Content(albums)">$INFO[Container(513).ListItem.DBID,musicdb://albums/,/]</value>
		<value>musicdb://artists/$INFO[ListItem.DBID]/*</value>
	</variable>
	<variable name="ArtistInfoPathVar">
		<value condition="!System.GetBool(musiclibrary.showcompilationartists)">musicdb://artists/$INFO[ListItem.DBID]/?albumartistsonly=true</value>
		<value>musicdb://artists/$INFO[ListItem.DBID]/?albumartistsonly=false</value>
	</variable>
	<variable name="ArtistAlbumInfoPathVar">
		<value condition="!System.GetBool(musiclibrary.showcompilationartists)">musicdb://artists/$INFO[ListItem.DBID]/$INFO[Window(musicinformation).Property(AlbumDBID)]/?albumartistsonly=true&amp;artistid=$INFO[ListItem.DBID]</value>
		<value>musicdb://artists/$INFO[ListItem.DBID]/$INFO[Window(musicinformation).Property(AlbumDBID)]/?albumartistsonly=false&amp;artistid=$INFO[ListItem.DBID]</value>
	</variable>
	<variable name="ListContentVar">
		<value condition="String.IsEmpty(Container.PluginName)">$INFO[Container(50).ListItem.FolderPath]</value>
		<value />
	</variable>
	<variable name="WeatherIconsPathVar">
		<value condition="!String.IsEmpty(Skin.String(weathericons.path))">$INFO[Skin.String(weathericons.path)]</value>
		<value>resource://resource.images.weathericons.default/</value>
	</variable>
	<variable name="MusicGenreIconsPathVar">
		<value condition="!String.IsEmpty(Skin.String(musicgenreicons.path))">$INFO[Skin.String(musicgenreicons.path)]</value>
		<value condition="!String.IsEmpty(Skin.String(MusicGenreIconPath))">$INFO[Skin.String(MusicGenreIconPath)]</value>
	</variable>
	<variable name="PlaylistStatsContentVar">
		<value condition="String.IsEqual(Container(9000).ListItem.Property(InfoLine),$LOCALIZE[31097]) | String.IsEqual(Container(9000).ListItem.Property(InfoLine),$LOCALIZE[31098]) | String.IsEqual(Container(9000).ListItem.Property(InfoLine),$LOCALIZE[31099])">Plugin://script.skin.info.service/?action=path_stats&amp;path=$INFO[Container(9000).ListItem.Property(list)]&amp;reload=$INFO[Window(Home).Property(SkinInfo.Library.Refreshed)]</value>
	</variable>
	<variable name="MediaFlagsPathVar">
		<value condition="Skin.HasSetting(coloredflags)">flags/colored</value>
		<value>flags/default</value>
	</variable>
	<variable name="PathFlagsStudios">
		<value condition="Skin.HasSetting(coloredflags)">resource://resource.images.studios.coloured/</value>
		<value>resource://resource.images.studios.white/</value>
	</variable>
	<variable name="PathFlagsLanguage">
		<value>flags/language/default/</value>
	</variable>
	<variable name="CaseBackPathVar">
		<value condition="!Skin.HasSetting(useclearcases)">thumbs/boxes/default/back/</value>
		<value condition="Skin.HasSetting(useclearcases)">thumbs/boxes/clearcases/back/</value>
	</variable>
	<variable name="CaseTypePathVar">
		<value condition="!Skin.HasSetting(useclearcases)">thumbs/boxes/default/</value>
		<value condition="Skin.HasSetting(useclearcases)">thumbs/boxes/clearcases/</value>
	</variable>
	<variable name="SeasonsBackgroundPathVar">
		<value condition="String.IsEqual(Skin.String(SeasonsBackground),MakeItSnow)">snow/snow</value>
		<value condition="String.IsEqual(Skin.String(SeasonsBackground),AutumnLeaves)">leaf/leaf</value>
	</variable>
</includes>