				<label>$LOCALIZE[5]$VAR[DialogSettingsHeaderVar]</label>
				<align>center</align>
				<textcolor>dialogheader</textcolor>
			</control>
			<control type="grouplist" id="90200">
				<left>1</left>
				<top>59</top>
				<width>1498</width>
				<height>660</height>
				<pagecontrol>6</pagecontrol>
				<onup>90200</onup>
				<ondown>90200</ondown>
				<onleft>6</onleft>
				<onright>6</onright>
